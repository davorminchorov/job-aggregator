{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "class_attributes_separation": {
            "elements": {
                "method": "one"
            }
        },
        "no_unused_imports": true,
        "ordered_imports": true,
        "single_quote": true,
        "trailing_comma_in_multiline": true
    }
}
